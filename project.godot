config_version=4

_global_script_classes=[ {
"base": "Control",
"class": "DialogueBlock",
"language": "GDScript",
"path": "res://src/blocks/dialogue_block.gd"
} ]
_global_script_class_icons={
"DialogueBlock": ""
}

[application]

config/name="Dialogue Editor"
run/main_scene="res://scenes/0.tscn"
run/low_processor_mode=true
boot_splash/image="res://sprites/loading_screen.png"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://sprites/icons/trash.png"

[autoload]

CSharp="*res://src/csharp/csharp.gd"
System="*res://src/utils/system.gd"
MainCamera="*res://src/main_camera.tscn"
Editor="*res://src/dialogue_editor.tscn"

[display]

window/size/width=1280
window/size/height=720
window/dpi/allow_hidpi=true
window/vsync/use_vsync=false
window/energy_saving/keep_screen_on=false
window/stretch/mode="2d"
window/stretch/aspect="expand"
window/tooltip_delay_sec=0.5

[global]

_global_script_classes=[  ]

[gui]

timers/text_edit_idle_detect_sec=20
theme/custom_font="res://fonts/NotoSans_16.tres"
timers/tooltip_delay_sec=0.5

